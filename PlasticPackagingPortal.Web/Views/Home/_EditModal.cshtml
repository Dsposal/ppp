                        <label asp-for="RecycledContentEvidenceReference"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Recyclability" class="form-control" placeholder="optional" type="text">
                        <label asp-for="Recyclability"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="RecyclabilitySource" class="form-control" placeholder="optional" type="text">
                        <label asp-for="RecyclabilitySource"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="RecyclabilityDate" class="form-control datepicker" placeholder="optional" type="text">
