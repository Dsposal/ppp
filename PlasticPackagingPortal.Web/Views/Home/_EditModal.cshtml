                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Colour" class="form-control" placeholder="optional" type="text">
                        <label asp-for="Colour"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Opacity" class="form-control" placeholder="optional" type="text">
                        <label asp-for="Opacity"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Level" class="form-control" placeholder="optional" type="text">
                        <label asp-for="Level"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="ReuseSystem" class="form-control" placeholder="optional" type="text">
                        <label asp-for="ReuseSystem"></label>
                    </div>
