@model PlasticPackagingPortal.Web.Models.Dto.StagingItemDto


<div class="modal fade" id="editRowModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="editRowModal" aria-hidden="true">
    <div class="modal-dialog modal-fullscreen-md-down modal-dialog-centered">
        <div class="modal-content">
            <form id="editRowForm" asp-action="UpdateStagingItem"asp-controller="Home" asp-antiforgery="true" method="post" role="form" data-get="@Url.Action("OnFormLoad", "Home")">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Edit Item</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <input asp-for="Id" type="hidden"/>
                    <div class="form-floating mb-3">
                        <input asp-for="Identifier" class="form-control" placeholder="optional">
                        <label asp-for="Identifier"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Name" class="form-control" placeholder="required" required>
                        <label asp-for="Name"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Description" class="form-control" placeholder="optional">
                        <label asp-for="Description"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="ImageUrl" class="form-control" placeholder="optional">
                        <label asp-for="ImageUrl"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="LowCode" class="form-control" placeholder="optional">
                        <label asp-for="LowCode"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Height" class="form-control" placeholder="optional">
                        <label asp-for="Height"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="HeightDate" class="form-control datepicker" placeholder="optional" type="text">
                        <label asp-for="HeightDate"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Width" class="form-control" placeholder="optional" type="text">
                        <label asp-for="Width"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="WidthDate" class="form-control datepicker" placeholder="optional" type="text">
                        <label asp-for="WidthDate"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Depth" class="form-control" placeholder="optional" type="text">
                        <label asp-for="Depth"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="DepthDate" class="form-control datepicker" placeholder="optional" type="text">
                        <label asp-for="DepthDate"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Volume" class="form-control" placeholder="optional" type="text">
                        <label asp-for="Volume"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="VolumeDate" class="form-control datepicker" placeholder="optional" type="text">
                        <label asp-for="VolumeDate"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Weight" class="form-control" placeholder="optional" type="text">
                        <label asp-for="Weight"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="WeightDate" class="form-control datepicker" placeholder="optional" type="text">
                        <label asp-for="WeightDate"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="WeightTolerance" class="form-control" placeholder="optional">
                        <label asp-for="WeightTolerance"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Function" class="form-control" placeholder="optional" type="text">
                        <label asp-for="Function"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="Shape" class="form-control" placeholder="optional" type="text">
                        <label asp-for="Shape"></label>
                    </div>
