                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="UpdateDate" class="form-control datepicker" placeholder="optional" type="text">
                        <label asp-for="UpdateDate"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="ReleaseDate" class="form-control datepicker" placeholder="optional" type="text">
                        <label asp-for="ReleaseDate"></label>
                    </div>
                    <div class="form-floating mb-3">
                        <input asp-for="DiscontinueDate" class="form-control datepicker" placeholder="optional" type="text">
                        <label asp-for="DiscontinueDate"></label>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-tertiary">Save changes</button>
                </div>
            </form>
        </div>
    </div>
</div>